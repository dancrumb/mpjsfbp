{
  "name": "jsfbp",
  "version": "1.3.0",
  "description": "FBP implementation written using node-fibers ",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpaulm/jsfbp.git"
  },
  "engines": {
    "node": ">= 0.12.7"
  },
  "keywords": [
    "fbp",
    "flow-based-programming",
    "node-fibers"
  ],
  "author": "J. Paul Morrison",
  "contributors": [
    "Dan Rumney <dancrumb@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpaulm/jsfbp/issues"
  },
  "homepage": "https://github.com/jpaulm/jsfbp",
  "dependencies": {
    "bunyan": "^1.8.1",
    "enumify": "^1.0.4",
    "fibers": "^1.0.5",
    "lodash": "^4.13.1",
    "parsefbp": "^0.3.0",
    "synchronize": "^2.0.0"
  },
  "main": "index.js",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "docdash": "^0.4.0",
    "dummy-module": "*",
    "eslint": "^2.10.2",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-es2015": "^1.1.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "flow-bin": "^0.33.0",
    "glob": "^7.1.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "js-beautify": "^1.6.3",
    "jsdoc": "^3.4.2",
    "mocha": "^2.1.0",
    "mocha-fibers": "^1.1.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "ws": "^1.1.0"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "compile": "babel --source-maps  --presets es2015 --plugins add-module-exports -d lib/ src/",
    "buildMochaOpts": "babel-node buildMochaOpts.js > test/mocha.opts",
    "test": "babel-node node_modules/.bin/isparta cover _mocha",
    "generate-docs": "jsdoc -c jsdoc.json -R README.md",
    "lint": "eslint src/core src/components examples test index.js",
    "ws": "js-beautify -r -f src/core/*.js src/components/*.js"
  }
}
